#include <stdio.h>
#define tam 8

int main() {
  int vetorid[tam]={1, 2, 3, 4, 5, 6, 7, 8};
  int vetoraluguel[tam];
  int i, gratis;

  for(i=0; i<tam; i++){
    printf("Matricula %d: ", vetorid[i]);
    scanf("%d", &vetoraluguel[i]);
  }
  for(i=0; i<tam; i++){
    gratis = vetoraluguel[i] / 10;
    printf("O Cliente %d tem %d filmes gratis \n", vetorid[i], gratis);

  }

  return 0;
}
